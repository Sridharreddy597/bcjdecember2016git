package com.bcj.corejava.oops.interfaces.lab2;

public class CharSequenceMethods implements CharSequence {

	@Override
	public char charAt(int arg0) {
		// TODO Auto-generated method stub
		String name="sridhar reddy ramasahayam";  
		char ch=name.charAt(arg0);//returns the char value at the 10th index  
		 

		return ch;
	}

	@Override
	public int length() {
		// TODO Auto-generated method stub
		String name="sridhar reddy ramasahayam";  
		int length =name.length();
		return length;
	}

	@Override
	public CharSequence subSequence(int arg0, int arg1) {
		String name="sridhar reddy ramasahayam";  
		
		return name.substring(arg0, arg1);
	}

}
