		<div>
			<button type="button" id="get" class="btn btn-primary btn-md">get</button>
			<div id="div2"></div>
			<br>
		</div>
	</div>



	<script>


    $("#post").on('click', function(){
    event.preventDefault();
        $('#div1').load('postfrm')
         $('#div1').toggle();
    });



    $("#get").on('click',function(){
        $('#div2').load('getfrm')
         $('#div2').toggle();
    });





</script>



